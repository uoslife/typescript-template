{
  "private": "true",
  "name": "uoslife-typescript-template",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "UOSLIFE <dev@uoslife.team>",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "cross-env NODE_ENV=development node .",
    "start:prod": "cross-env NODE_ENV=prod node .",
    "serve": "yarn serve:dev",
    "serve:dev": "cross-env NODE_ENV=development tsc-watch --onSuccess \"node .\"",
    "serve:prod": "cross-env NODE_ENV=prod tsc-watch --onSuccess \"node .\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.3",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^16.11.41",
    "@types/prettier": "^2.6.3",
    "@types/tsc-watch": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
